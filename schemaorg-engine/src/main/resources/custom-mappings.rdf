<?xml version="1.0" encoding="utf-8"?>
<rdf:RDF xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#" xmlns:owl="http://www.w3.org/2002/07/owl#" xmlns:xsd="http://www.w3.org/2001/XMLSchema#" xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#" xmlns="http://dbpedia.org/ontology/" xml:base="http://dbpedia.org/ontology/">

  <owl:Class rdf:about="http://schema.org/Person">
    <rdfs:subClassOf rdf:resource="http://schema.org/Thing"/>
  </owl:Class>

  <owl:Class rdf:about="http://schema.org/GeoCoordinates">
    <rdfs:subClassOf rdf:resource="http://schema.org/Thing"/>
  </owl:Class>

  <!--
  <owl:DatatypeProperty rdf:about="http://schema.org/name">
    <rdfs:domain rdf:resource="http://schema.org/Thing"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="http://schema.org/description">
    <rdfs:domain rdf:resource="http://schema.org/Thing"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
  </owl:DatatypeProperty>
  -->
  
  <owl:DatatypeProperty rdf:about="http://www.w3.org/2000/01/rdf-schema#label">
    <rdfs:domain rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <owl:equivalentProperty rdf:resource="http://schema.org/name"/>
  </owl:DatatypeProperty>
  
  <owl:DatatypeProperty rdf:about="http://dbpedia.org/ontology/abstract">
    <rdfs:domain rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <owl:equivalentProperty rdf:resource="http://schema.org/description"/>
  </owl:DatatypeProperty>
  
  <owl:DatatypeProperty rdf:about="http://www.w3.org/2000/01/rdf-schema#comment">
    <rdfs:domain rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <owl:equivalentProperty rdf:resource="http://schema.org/description"/>
  </owl:DatatypeProperty>

  <owl:DatatypeProperty rdf:about="http://www.w3.org/2000/01/rdf-schema#comment">
    <rdfs:domain rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <owl:equivalentProperty rdf:resource="http://schema.org/description"/>
  </owl:DatatypeProperty>

  <owl:ObjectProperty rdf:about="http://dbpedia.org/ontology/thumbnail">
    <rdfs:domain rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
    <owl:equivalentProperty rdf:resource="http://schema.org/image"/>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="http://xmlns.com/foaf/0.1/depiction">
    <rdfs:domain rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
    <owl:equivalentProperty rdf:resource="http://schema.org/image"/>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="http://dbpedia.org/property/website">
    <rdfs:domain rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
    <owl:equivalentProperty rdf:resource="http://schema.org/url"/>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="http://xmlns.com/foaf/0.1/homepage">
    <rdfs:domain rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
    <owl:equivalentProperty rdf:resource="http://schema.org/url"/>
  </owl:ObjectProperty>
 
  <owl:ObjectProperty rdf:about="http://dbpedia.org/property/currentTeam">
    <rdfs:domain rdf:resource="http://dbpedia.org/ontology/Person"/>
    <owl:equivalentProperty rdf:resource="http://schema.org/affiliation"/>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="http://dbpedia.org/property/shortDescription">
    <rdfs:domain rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
    <owl:equivalentProperty rdf:resource="http://schema.org/jobTitle"/>
  </owl:ObjectProperty>
    
  <owl:DatatypeProperty rdf:about="http://www.w3.org/2003/01/geo/wgs84_pos#lat">
    <rdfs:domain rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
    <!-- rdfs:domain rdf:resource="http://schema.org/GeoCoordinates"/ -->
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#double"/>
    <owl:equivalentProperty rdf:resource="http://schema.org/latitude"/>
  </owl:DatatypeProperty>

  <owl:DatatypeProperty rdf:about="http://www.w3.org/2003/01/geo/wgs84_pos#long">
    <rdfs:domain rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
    <rdfs:domain rdf:resource="http://schema.org/GeoCoordinates"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#double"/>
    <owl:equivalentProperty rdf:resource="http://schema.org/longitude"/>
  </owl:DatatypeProperty>

    
</rdf:RDF>
